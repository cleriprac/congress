<h3>Upcoming Bills</h3>

<table class="table table-striped table-style">
  <thead>
  <tr>
    <th width="120">Legislative Day</th>
    <th width="400">Bill Description</th>
  </tr>
  </thead>
  <tbody>
  {{#each bill in model}}
  <tr>
    <td>{{bill.legislative_day}}</td>
    <td>{{bill.description}} </td>
  </tr>
  {{/each}}
  </tbody>
</table>
